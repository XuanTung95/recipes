[
  {
    "cmd": [
      "ls",
      "-la"
    ],
    "name": "mytest",
    "timeout": 3600,
    "~followup_annotations": [
      "@@@STEP_LOG_LINE@stdout@#flaky@@@",
      "@@@STEP_LOG_LINE@stdout@this is a flaky@@@",
      "@@@STEP_LOG_LINE@stdout@flaky: true@@@",
      "@@@STEP_LOG_END@stdout@@@",
      "@@@STEP_LOG_LINE@stderr@@@@",
      "@@@STEP_LOG_END@stderr@@@"
    ]
  },
  {
    "name": "$result"
  }
]